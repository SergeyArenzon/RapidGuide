import { z } from 'zod';
export declare const createGuideSchema: z.ZodObject<{
    name: z.ZodString;
    bio: z.ZodString;
    country_code: z.ZodString;
    city_id: z.ZodNumber;
    languages_code: z.ZodArray<z.ZodString>;
    subcategories_id: z.ZodArray<z.ZodUUID>;
}, z.core.$strip>;
export type CreateGuideDto = z.infer<typeof createGuideSchema>;
